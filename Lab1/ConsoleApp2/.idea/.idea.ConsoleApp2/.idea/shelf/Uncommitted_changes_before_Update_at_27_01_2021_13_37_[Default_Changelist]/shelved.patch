l;\r\n        public List<Point> AreaPoints;\r\n\r\n        public Area(Point kernel)\r\n        {\r\n            OldKernel = new Point(-1, -1);\r\n            AreaPoints = new List<Point>();\r\n            Kernel = kernel;\r\n        }\r\n    }\r\n}
